#include<iostream>
#include<vector>
using namespace std;

int sum(vector<int>v){
    int sum=0;
    for(int i=0;i<v.size();i++){
        sum+=v[i];
        
    }
    cout<<sum;
    return 0;
}
int main(){
    int n;
    cin>>n;
    int element;
    vector<int>A;
    for(int i=0;i<n;i++){
        cin>>element;
        A.push_back(element);
        
    }
    sum(A);
    
    
    return 0;
}
