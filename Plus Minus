#include<iostream>
#include<vector>
#include<iomanip>
using namespace std;

int frac(vector<int>v , int a){
    int c1=0,c2=0,c3=0;
    for(int i=0;i<v.size();i++){
    if(v[i]>0){
        c1++;
    }  
    else if(v[i]<0){
        c2++;
    }  
     else if(v[i]==0){
         c3++;
     }   }
    cout<<fixed<<setprecision(6)<<(double)c1/a<<endl;
    cout<<fixed<<setprecision(6)<<(double)c2/a<<endl;
    cout<<fixed<<setprecision(6)<<(double)c3/a<<endl;
    return 0;
} 

int main(){
    int n;
    cin>>n;
    vector<int>A;
    int element;
    for(int i=0;i<n;i++){
        cin>>element;
        A.push_back(element);
        
    }
    frac(A , n);
    
    
    return 0;
}
